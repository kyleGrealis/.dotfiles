[Desktop Entry]
Name=Firefox Nightly
Comment=Firefox Developer Edition
Exec=env GDK_BACKEND=wayland MOZ_ENABLE_WAYLAND=1 /home/kyle/firefox/firefox %u
Icon=/home/kyle/firefox/browser/chrome/icons/default/default128.png
StartupWMClass=firefox-nightly
Terminal=false
Type=Application
Categories=Network;WebBrowser;
MimeType=text/html;text/xml;application/xhtml+xml;application/xml;application/rss+xml;application/rdf+xml;x-scheme-handler/http;x-scheme-handler/https;
Actions=new-window;new-private-window;

[Desktop Action new-window]
Name=New Window
Exec=env GDK_BACKEND=wayland MOZ_ENABLE_WAYLAND=1 /home/kyle/firefox/firefox --new-window %u

[Desktop Action new-private-window]
Name=New Private Window
Exec=env GDK_BACKEND=wayland MOZ_ENABLE_WAYLAND=1 /home/kyle/firefox/firefox --private-window %u